{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tutorial/blog-list-js",
	"version": "0.1.0",
	"title": "JS Listing",
	"category": "widgets",
	"icon": "list-view",
	"description": "Will use JS instead of Render.php",
	"example": {},
	"supports": {
		"html": false,
		"color":{
			"background": true,
			"text": true
		},
		"spacing": {
			"padding": true
		}
	},
	"attributes": {
		"settings": {
			"type": "object",
			"default": {
				"post_type": "post",
				"posts_per_page": "2"
			}
		}
	},
	"textdomain": "blog-list-js",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}